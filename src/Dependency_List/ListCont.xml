<?xml version="1.0" encoding="UTF-8"?>

<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  

<bean id="que" class="Dependency_List.Question">
<property name="Sid" value="1"></property>
<property name="Name" value="Ahmed Alraz"></property>
<property name="answer">
<list>
<value>Java</value>
<value>Java2s</value>
<value>Java4s</value>
</list>

</property>
</bean>

<bean id="que1" class="Dependency_List.setQue">
<property name="Sid" value="1"></property>
<property name="Name" value="Ahmed Alraz"></property>
<property name="answer">
<set>
<value>Java2s</value>
<value>Java</value>
<value>Java2s</value>
<value>Java4s</value>
</set>
</property>
</bean>


<bean id="que2" class="Dependency_List.MapQue">
<property name="Sid" value="1"></property>
<property name="Name" value="Ahmed Alraz"></property>
<property name="answer">
<map >
<entry key="k1" value="java21"></entry>
<entry key="k2" value="java22"></entry>
<entry key="k3" value="java23"></entry>

</map>

</property>
</bean>






</beans>